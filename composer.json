{
	"name": "typo3/cms-base-distribution",
	"description" : "TYPO3 CMS Base Distribution",
	"license": "GPL-2.0-or-later",
	"type": "project",
	"config": {
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true,
			"helhum/dotenv-connector": true
		},
		"platform": {
			"php": "8.1.1"
		},
		"sort-packages": true
	},
	"repositories": [
		{
			"type": "path",
			"url": "./packages/*"
		}
	],
	"require": {
		"florian-geierstanger/typo3-vite-demo": "^0.0.1",
		"helhum/dotenv-connector": "^3.0",
		"praetorius/vite-asset-collector": "dev-main",
		"typo3/cms-backend": "^12.1.0",
		"typo3/cms-belog": "^12.1.0",
		"typo3/cms-beuser": "^12.1.0",
		"typo3/cms-core": "^12.1.0",
		"typo3/cms-dashboard": "^12.1.0",
		"typo3/cms-extbase": "^12.1.0",
		"typo3/cms-extensionmanager": "^12.1.0",
		"typo3/cms-felogin": "^12.1.0",
		"typo3/cms-filelist": "^12.1.0",
		"typo3/cms-fluid": "^12.1.0",
		"typo3/cms-fluid-styled-content": "^12.1.0",
		"typo3/cms-form": "^12.1.0",
		"typo3/cms-frontend": "^12.1.0",
		"typo3/cms-impexp": "^12.1.0",
		"typo3/cms-info": "^12.1.0",
		"typo3/cms-install": "^12.1.0",
		"typo3/cms-lowlevel": "^12.2",
		"typo3/cms-reactions": "^12.2",
		"typo3/cms-redirects": "^12.2",
		"typo3/cms-rte-ckeditor": "^12.1.0",
		"typo3/cms-scheduler": "^12.2",
		"typo3/cms-seo": "^12.1.0",
		"typo3/cms-setup": "^12.1.0",
		"typo3/cms-sys-note": "^12.1.0",
		"typo3/cms-t3editor": "^12.1.0",
		"typo3/cms-tstemplate": "^12.1.0",
		"typo3/cms-viewpage": "^12.1.0"
	}
}
